{
  "name"       : "tapakan/di",
  "description": "PHP Dependency Injection container",
  "version"    : "0.0.1",
  "keywords"   : [
    "di",
    "dependency injection",
    "inversion of control",
    "ioc",
    "dic"
  ],
  "homepage"   : "https://github.com/Tapakan/di.git",
  "license"    : "MIT",
  "authors"    : [
    {
      "name" : "Alexander Oganov",
      "email": "t_tapak@yahoo.com",
      "role" : "Developer"
    }
  ],
  "require"    : {
    "php": ">=5.6"
  },
  "require-dev": {
    "codeception/codeception"  : "^2.2",
    "codeception/verify"       : "~0.3.1",
    "codeception/specify"      : "~0.4.5",

    "squizlabs/php_codesniffer": "~2.8.0",
    "satooshi/php-coveralls"   : "~1.0",

    "phpunit/php-code-coverage": "~4.0"
  },
  "autoload"   : {
    "psr-4": {
      "Tapakan\\Di\\"           : "src",
      "Tapakan\\Di\\Exception\\": "src/Exception",
      "Tapakan\\Di\\Tests\\"    : "tests"
    }
  },
  "extra"      : {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}